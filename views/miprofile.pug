    // Main
    article#main
      .row.gtr-50.gtr-uniform
        .col-12#banner
          span.image.fit
            h2.center Mi Masterpiece's
            #myCarousel.carousel.slide(data-ride='carousel')
              // Indicators
              ol.carousel-indicators
                li.active(data-target='#myCarousel', data-slide-to='0')
                li(data-target='#myCarousel', data-slide-to='1')
                li(data-target='#myCarousel', data-slide-to='2')
              // Wrapper for slides
              .carousel-inner
                .item.active
                  img(src='https://healthykitchen.es/wp-content/uploads/2018/10/Cauliflower-Rice-1200x700.jpg', alt='Dinner', style='width:100%;')
                .item
                  img(src='http://lifestylepubs.wordpress.s3-us-west-2.amazonaws.com/app/uploads/prod/sites/107/2018/12/IMG_5660..jpg')
